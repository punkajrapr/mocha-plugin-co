  "description": "Mocha plugin to enable support for generators (using co)",
  "main": "install.js",
  "keywords": [
    "co",
    "mocha",
    "generators"
  ],
  "license": "MIT",
  "dependencies": {
    "co": "^4.0.0"
  },
